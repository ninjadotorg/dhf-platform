{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 9000,
  "remoting": {
    "context": false,
    "cors": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    }
  },
  "crons": {
    "firstReleased": {
      "enable": true,
      "jobClass": "lib/firstReleased-job",
      "interval": "0    *     * * * *"
    }
  }
}
