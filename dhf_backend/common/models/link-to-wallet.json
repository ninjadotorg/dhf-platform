{
  "name": "link-to-wallet",
  "plural": "link-to-wallet",
  "base": "PersistedModel",
  "idInjection": true,
  "strict": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "walletId": {
      "type": "string",
      "required": true
    },
    "wallets": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "emun": ["pending", "approved", "rejected"]
    },
    "activeDate": {
      "type": "date"
    },
    "requestDate": {
      "type": "date"
    },
    "verifyCode": {
      "id":true,
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "User": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {},
  "mixins": {
    "DisableMethods": {
      "expose": [
        "create",
        "isLinked",
        "verify"
      ]
    }
  }
}
