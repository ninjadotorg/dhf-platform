{
  "name": "project",
  "base": "PersistedModel",
  "idInjection": true,
  "hidden": ["UserId"],
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "owner": {
      "type": "string",
      "required": true
    },
    "target": {
      "type": "number",
      "required": true
    },
    "exchange": {
      "type": "string",
      "required": true
    },
    "max": {
      "type": "number"
    },
    "fundingAmount": {
      "type": "number"
    },
    "updatedAmmount": {
      "type": "number"
    },
    "releasedAmount": {
      "type": "number"
    },
    "pendingAmount": {
      "type": "number"
    },
    "refundAmount": {
      "type": "number"
    },
    "startTime": {
      "type": "number",
      "required": true
    },
    "deadline": {
      "type": "number",
      "required": true
    },
    "lifeTime": {
      "type": "number"
    },
    "funds": {
      "type": "object"
    },
    "cancelRequests": {
      "type": "object"
    },
    "state": {
      "type": "string",
      "required": true,
      "enum": [
        "INITFUND",
        "APPROVED",
        "READY",
        "RELEASEED",
        "TOP",
        "WITHDRAW"
      ]
    }
  },
  "validations": [],
  "relations": {
    "User": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "",
      "required": true
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "listProjects"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "trader",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "trader",
      "permission": "ALLOW",
      "property": "myProjects"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {},
  "mixins": {
    "DisableMethods": {
      "expose": [
        "create",
        "findById",
        "deleteById",
        "replaceById",
        "listProjects",
        "myProjects"
      ]
    }
  }
}
