{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "strict": true,
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "userType": {
      "type": "string",
      "enum": [
        "user",
        "trader",
        "admin"
      ]
    }
  },
  "hidden": ["realm"],
  "relations": {
    "project": {
      "type": "hasMany",
      "model": "project",
      "foreignKey": ""
    },
    "funding": {
      "type": "hasMany",
      "model": "funding",
      "foreignKey": ""
    },
    "contract": {
      "type": "hasMany",
      "model": "link-to-contract",
      "foreignKey": ""
    },
    "accessToken": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "listTrader"
    }
  ],
  "methods": {},
  "mixins": {
    "DisableMethods": {
      "expose": [
        "create",
        "findById",
        "deleteById",
        "replaceById",
        "login",
        "logout",
        "listTrader"
      ]
    }
  }
}
