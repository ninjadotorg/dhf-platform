{
  "name": "bootstrap-webpack-jquery-boilerplate",
  "license": "MIT",
  "author": "xxhomey19",
  "homepage": "https://xxhomey19.github.io/bootstrap-webpack-jquery-boilerplate/",
  "repository": {
    "type": "git",
    "url": "https://github.com/xxhomey19/bootstrap-webpack-jquery-boilerplate/tree/master"
  },
  "bugs": {
    "url": "https://github.com/xxhomey19/bootstrap-webpack-jquery-boilerplate/issues"
  },
  "version": "1.2.2",
  "scripts": {
    "build": "webpack -p --progress --mode production --config webpack.config.js --host 0.0.0.0",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "cross-env NODE_ENV=dev webpack-serve --open --config webpack.config.js --host 0.0.0.0",
    "prod": "cross-env NODE_ENV=PROD webpack-serve --open --config webpack.config.js",
    "lint": "eslint src",
    "scss": "node-sass --watch scss -o css",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "bootstrap": "^4.1.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "webpack-livereload-plugin": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jquery": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "gh-pages": "^1.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.3.1",
    "install": "^0.12.1",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.13.7",
    "prettier-package-json": "^1.6.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
